void sumaBin(int num1, int num2, int result);
void valoresVariables(int num1, int num2);

void main()
{
   Set_tris_c(0x00);
   Set_tris_b(0xff);

   
   int1 state;
   int numero = 0;
   
   while(TRUE)
   {
      numero = ()input_b();
      output_c(numero);
      numero=0;
   }
}


